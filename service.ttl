@prefix notary: <http://example.org/notary#>.
@prefix nservice: <http://example.org/nservice#>.

nservice:name rdf:type rdf:Property ;
    rdfs:label "name" ;
    rdfs:domain notary:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of the service." .

nservice:description rdf:type rdf:Property ;
    rdfs:label "description" ;
    rdfs:domain notary:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "A short description of the service" .

nservice:SN1 a notary:Service;
    nservice:name "Autentificarea actelor";
    nservice:description "serviciu prin care un notar atesta autenticitatea unei semnaturi sau a unui document." .

nservice:SN2 a notary:Service;
    nservice:name "Eliberarea de certificate de mostenitor";
    nservice:description "serviciu prin care un notar elibereaza un document care atesta dreptul unei persoane de a mosteni un bun." .

nservice:SN3 a notary:Service;
    nservice:name "Inscrierea de ipoteca";
    nservice:description "serviciu prin care un notar inregistreaza o ipoteca asupra unui bun imobil." .

nservice:SN4 a notary:Service;
    nservice:name "Eliberarea de certificate constatatoare";
    nservice:description "serviciu prin care un notar elibereaza un document care atesta starea unui bun (de exemplu, proprietatea sau posesia unui bun)." .

nservice:SN5 a notary:Service;
    nservice:name "Redactarea de acte notariale";
    nservice:description "serviciu prin care un notar redacteaza acte juridice (de exemplu, contracte de vanzare-cumparare sau testamente)." .

nservice:SN6 a notary:Service;
    nservice:name "Legalizarea de semnaturi";
    nservice:description "serviciu prin care un notar atesta ca o semnatura este autentica." .

nservice:SN7 a notary:Service;
    nservice:name "Inscrierea de uzufruct";
    nservice:description "serviciu prin care un notar inregistreaza un drept de uzufruct asupra unui bun." .

nservice:SN8 a notary:Service;
    nservice:name "Eliberarea de copii legalizate";
    nservice:description "serviciu prin care un notar elibereaza o copie legalizata a unui document." .

nservice:SN9 a notary:Service;
    nservice:name "Inscrierea de servitute";
    nservice:description "serviciu prin care un notar inregistreaza o servitute asupra unui bun." .

nservice:SN10 a notary:Service;
    nservice:name "Depozitarea de documente";
    nservice:description "serviciu prin care un notar pune la dispozitie o locatie sigura pentru depozitarea documentelor importante." .

nservice:SN11 a notary:Service;
    nservice:name "Transcrierea de acte";
    nservice:description "serviciu prin care un notar transcrie informatii dintr-un act intr-un alt act (de exemplu, transcrierea unui act de nastere intr-un act de casatorie)." .

nservice:SN12 a notary:Service;
    nservice:name "Autentificarea de traduceri";
    nservice:description "serviciu prin care un notar autentifica traducerea unui document din limba originala in alta limba." .

nservice:SN13 a notary:Service;
    nservice:name "Autentificarea de contracte";
    nservice:description "serviciu prin care un notar autentifica un contract intre doua sau mai multe parti." .

nservice:SN14 a notary:Service;
    nservice:name "Inscrierea de drept de proprietate";
    nservice:description "serviciu prin care un notar inregistreaza dreptul de proprietate asupra unui bun." .

nservice:SN15 a notary:Service;
    nservice:name "Eliberarea de certificate de nastere";
    nservice:description "serviciu prin care un notar elibereaza un document care atesta data si locul nasterii unei persoane." .

nservice:SN16 a notary:Service;
    nservice:name "Inscrierea de drept de folosinta";
    nservice:description "serviciu prin care un notar inregistreaza un drept de folosinta asupra unui bun." .

nservice:SN17 a notary:Service;
    nservice:name "Inscrierea de drept de superficie";
    nservice:description "serviciu prin care un notar inregistreaza un drept de superficie asupra unui bun." .

nservice:SN18 a notary:Service;
    nservice:name "Inscrierea de drept de preemtiune";
    nservice:description "serviciu prin care un notar inregistreaza un drept de preemtiune asupra unui bun." .

nservice:SN19 a notary:Service;
    nservice:name "Inregistrarea de societati comerciale";
    nservice:description "serviciu prin care un notar inregistreaza o societate comerciala si emite actele necesare pentru functionarea acesteia." .

nservice:SN20 a notary:Service;
    nservice:name "Inscrierea de drept de concesiune";
    nservice:description "serviciu prin care un notar inregistreaza un drept de concesiune asupra unui bun." .

nservice:SN21 a notary:Service;
    nservice:name "Autentificarea de procuri";
    nservice:description "serviciu prin care un notar autentifica o procura, care este un document prin care o persoana da altcuiva puterea de a lua decizii sau de a incheia acte in numele sau." .

nservice:SN23 a notary:Service;
    nservice:name "Altele";
    nservice:description "Alte servicii notariale." .