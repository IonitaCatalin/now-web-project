@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix now: <http://example.org/now#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .


now:Notary rdf:type rdfs:Class ;
    rdfs:label "Notary" .

now:Translator rdf:type rdfs:Class ;
    rdfs:label "Translator" .

now:Service rdf:type rdfs:Class ;
    rdfs:label "Service" .

now:Rating rdf:type rdfs:Class ;
    rdfs:label "Rating" .

now:name rdf:type rdf:Property ;
    rdfs:label "name" ;
    rdfs:domain now:Notary, now:Tranlator;
    rdfs:range xsd:string ;
    rdfs:comment "The name of the notary." .

now:room rdf:type rdf:Property ;
    rdfs:label "room" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "The room number where the notary is located." .

now:address rdf:type rdf:Property ;
    rdfs:label "address" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "The street address of the notary." .

now:city rdf:type rdf:Property ;
    rdfs:label "city" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "The city where the notary is located." .

now:county rdf:type rdf:Property ;
    rdfs:label "county" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "The county where the notary is located." .

now:description rdf:type rdf:Property ;
    rdfs:label "description" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "A short description of the notary." .

now:phoneNumbers rdf:type rdf:Property ;
    rdfs:label "phoneNumbers" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range rdf:List ;
    rdfs:comment "A list of phone numbers associated with the notary." .

now:email rdf:type rdf:Property ;
    rdfs:label "email" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range xsd:string ;
    rdfs:comment "The email address of the notary." .

now:languages rdf:type rdf:Property ;
    rdfs:label "languages" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range rdf:List ;
    rdfs:comment "A list of languages spoken by the notary." .

now:coordinates a geo:asWKT;
    rdfs:label "coordinates"@en;
    rdfs:domain now:Notary, now:Tranlator;
    rdfs:comment "The geographic coordinates of the notary.".

now:appeal_court a geo:asWKT;
    rdfs:label "appeal_court";
    rdfs:domain now:Notary, now:Tranlator;
    rdfs:comment "The apeal court of which the institution belongs to.".

now:autorization_number a geo:asWKT;
    rdfs:label "autorization_number";
    rdfs:domain now:Notary, now:Tranlator;
    rdfs:comment "The authorization number of the institution.".

now:services rdf:type rdf:Property ;
    rdfs:label "services" ;
    rdfs:domain now:Notary, now:Tranlator ;
    rdfs:range rdf:List ;
    rdfs:comment "A list of services provided by the notary." .